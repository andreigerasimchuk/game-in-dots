{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "globals": {
      "document": true,
      "localStorage": true,
      "window": true,
      "fetch": true,
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
    ],
    "rules": {
      "react/prop-types": 0,
      "react/no-array-index-key": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/click-events-have-key-events": 0,
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "babel",
      "import"
    ]
  }
  